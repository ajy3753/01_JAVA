package numRange;

public class NumberController {
	// 기본 생성자
	public NumberController() {}
	
	// 메소드
	public boolean checkDouble(int num1, int num2) throws NumRangeException {
		// 매개변수로 들어온 두 값이 1 ~ 100 사이 일때 num1이 num2의 배수면 true, 아니라면 false 반환
		// 1 ~ 100 사이가 아니라면 NumRangeException 발생, 에러 메시지는 "1부터 100사이의 값이 아닙니다"
		
		return true;
	}
}
